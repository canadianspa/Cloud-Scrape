<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<servlet>
		<servlet-name>Scrape</servlet-name>
		<servlet-class>gxs.Scrape</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Scrape</servlet-name>
		<url-pattern>/scrape</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Email</servlet-name>
		<servlet-class>gxs.Emailer</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Email</servlet-name>
		<url-pattern>/email</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>DetectDelivery</servlet-name>
		<servlet-class>gxs.DetectDelivery</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DetectDelivery</servlet-name>
		<url-pattern>/delivery</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>logs</servlet-name>
		<servlet-class>gxs.LogOutput</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>logs</servlet-name>
		<url-pattern>/logs</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>statusReport</servlet-name>
		<servlet-class>gxs.currentStatusReport</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>statusReport</servlet-name>
		<url-pattern>/status</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>test</servlet-name>
		<servlet-class>gxs.test</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>test</servlet-name>
		<url-pattern>/test</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>setUp</servlet-name>
		<servlet-class>gxs.setUp</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>setUp</servlet-name>
		<url-pattern>/setup</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
