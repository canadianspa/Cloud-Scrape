<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<servlet>
		<servlet-name>HomeBaseScrape</servlet-name>
		<servlet-class>servlets.HomeBaseScrape</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>HomeBaseScrape</servlet-name>
		<url-pattern>/hscrape</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>hs</servlet-name>
		<servlet-class>servlets.startHomeBaseScrape</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>hs</servlet-name>
		<url-pattern>/hs</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>BNQScrape</servlet-name>
		<servlet-class>servlets.BNQScrape</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>BNQScrape</servlet-name>
		<url-pattern>/bscrape</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>bs</servlet-name>
		<servlet-class>servlets.startBNQScrape</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>bs</servlet-name>
		<url-pattern>/bs</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>logs</servlet-name>
		<servlet-class>servlets.LogOutput</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>logs</servlet-name>
		<url-pattern>/logs</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>test</servlet-name>
		<servlet-class>servlets.test</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>test</servlet-name>
		<url-pattern>/test</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>setUp</servlet-name>
		<servlet-class>servlets.setUp</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>setUp</servlet-name>
		<url-pattern>/setup</url-pattern>
	</servlet-mapping>
	<filter>
		<filter-name>ObjectifyFilter</filter-name>
		<filter-class>com.googlecode.objectify.ObjectifyFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>ObjectifyFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
	<listener>
		<listener-class>entities.OfyHelper</listener-class>
	</listener>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
