<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<servlet>
		<servlet-name>Scrape</servlet-name>
		<servlet-class>gxs.Scrape</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Scrape</servlet-name>
		<url-pattern>/scrape</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>toVeeqoServer</servlet-name>
		<servlet-class>gxs.toVeeqoServer</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>toVeeqoServer</servlet-name>
		<url-pattern>/vs</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>gcs</servlet-name>
		<servlet-class>gxs.GcsExampleServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>gcs</servlet-name>
		<url-pattern>/gs</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
